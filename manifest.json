{
    "name": "newTab",
    "version": "1.0",
    "manifest_version": 2,
    "description": "拡張機能です",
    "permissions": [
        "https://www.google.com/*",
        "bookmarks", "history", "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}